{
  "name": "junox",
  "version": "0.2.0",
  "license": "GPL-3.0-or-later",
  "repository": "github:pendragon-andyh/junox",
  "homepage": "https://github.com/pendragon-andyh/junox",
  "bugs": "https://github.com/pendragon-andyh/junox/issues",
  "author": "Andy Harman <andyh.at.pendragon@gmail.com>",
  "contributors": [
    {
      "name": "Daniele Zannotti"
    }
  ],
  "files": [
    "dist/"
  ],
  "main": "dist/synth.node.js",
  "scripts": {
    "start": "serve",
    "lint": "eslint .",
    "build:all": "npm run build:patches && npm run build:pretty && npm run build:esm",
    "build:patches": "node patches/Juno60.js",
    "build:pretty": "prettier --write src",
    "build:esm": "esbuild --bundle --outdir=dist --format=esm --sourcemap ./src/synth.worklet.js ./src/synth.node.js",
    "build:iife": "esbuild --bundle --outfile=dist/synth.node.old.js --format=iife --global-name=Junox ./dist/synth.node.js",
    "deploy:github": "gh-pages --dist .",
    "yarn:install": "yarn install",
    "yarn:install- force": "yarn install --force",
    "yarn:upgrade-latest": "yarn upgrade-interactive --latest",
    "yarn:audit": "yarn audit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern src/**/*.js"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "csv-parser": "^2.3.3",
    "esbuild": "^0.5.23",
    "eslint": "^7.4.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "serve": "^11.3.2"
  },
  "resolutions": {
    "**/lodash": "^4.17.0",
    "**/minimist": "^1.2.0"
  }
}
